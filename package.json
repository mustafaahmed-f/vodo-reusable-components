{
  "name": "vodo-react-components",
  "description": "React resusable components for VODO fronend developers",
  "version": "1.1.8",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mustafaahmed-f/vodo-react-components-npm-package"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "ui",
    "components",
    "buttons",
    "table",
    "navbar"
  ],
  "scripts": {
    "dev": "vite",
    "testLocal": "npm unlink vodo-react-components && npm run build && npm link",
    "build": "tsup",
    "build-css": "postcss src/styles/tailwind.css -o dist/assets/styles/tailwind.css",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copy-assets": "cpx \"src/assets/**/*\" dist/assets"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.101",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "esbuild-css-modules-plugin": "^3.1.2",
    "lucide-react": "^0.399.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
